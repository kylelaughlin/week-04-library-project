<% if @library.errors.any? %>
  <% @library.errors.full_messages.each do |m| %>
    <p>Error: <%= m %></p>
  <% end %>
<% end %>

<h1>Create a New Library</h1>

<form action="/libraries" method="post">
  Branch Name: <input type="text" name="branch_name"></br>
  Address: <input type="text" name="address"></br>
  Phone Number: <input type="text" name="phone_number"></br>
  <input type="submit" value="Create New Library">
</form>

<p><a href="/libraries">Back to Libraries Menu</a><p>
